{
  "name": "advent-of-code-boilerplate",
  "version": "1.1.0",
  "description": "Boilerplate workspace for advent of code puzzle",
  "main": "index.ts",
  "scripts": {
    "build": "rm -rf dist && tsc && npm run cp:inputs",
    "cp:inputs": "ts-node src/scripts/copyInputFiles.ts",
    "debug": "nodemon input 5",
    "dev": "nodemon input",
    "dev:test": "nodemon test",
    "init-day": "ts-node src/scripts/initNewDay/initNewDay.ts",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "node dist/index.js",
    "prepare": "husky install",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "author": "Francesco Maida",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "husky": "^7.0.4",
    "install": "^0.13.0",
    "jest": "^27.4.2",
    "nodemon": "^2.0.20",
    "npm": "^8.19.3",
    "prettier": "^2.5.0",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "D": "^1.0.0",
    "replace-in-file": "^6.3.2"
  }
}
